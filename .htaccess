Options All -Indexes

RewriteEngine On
RewriteRule ^([-a-zA-Z0-9/]+)$ index.php?pagina=$1

Header always set Content-Security-Policy: upgrade-insecure-requests

RewriteEngine On
RewriteCond %{HTTP_HOST} ^https://dueño.site\.com$  [OR] 
RewriteCond %{HTTP_HOST} ^www\.dueño.site\.com$  
RewriteCond %{HTTPS} !on 
RewriteRule ^(.*)$ https://dueño.site/$1  [R=301,L,NE] 